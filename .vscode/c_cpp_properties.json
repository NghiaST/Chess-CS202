            "compilerArgs": [
                "./graphics/lib -lsfml-graphics",
                "./graphics/lib -lsfml-window",
                "./graphics/lib -lsfml-system"
            ],
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}