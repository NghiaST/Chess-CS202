{
    "configurations": [
        {
            "name": "Chess Project",
            "includePath": [
                "${workspaceFolder}/**",
                "SFML-2.5.1/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "D:/MinGW/bin/g++.exe",
            "cStandard": "c17",
